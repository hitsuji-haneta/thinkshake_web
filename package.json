{
  "name": "thinkshake",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "hitsuji-haneta <s.takahashi@thinkshake.net>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "contentful": "^6.1.1",
    "contentful-management": "^5.1.4",
    "nuxt": "^1.0.0",
    "vue-markdown": "^2.2.4",
    "vue-scrollto": "^2.11.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.2.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^23.3.0",
    "pug": "2.0.0-beta6",
    "pug-loader": "^2.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^2.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
